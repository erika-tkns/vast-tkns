<h1>キューポイント編集</h1>

<%= form_for(@cuepoint) do |f| %>
  <%= f.label :name, '名前' %>
  <%= f.text_field :name %>
  
  <%= link_to 'VAST URL', vast_url(@cuepoint) %>
  
  <%= f.submit '保存' %>
<% end %>

  <%= link_to '中止', cuepoints_path %>
