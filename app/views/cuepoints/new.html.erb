<h1>キューポイント登録</h1>

<%= form_for @cuepoint, html: {class: 'form-horizontal'} do |f| %>
  <%= render "shared/error_messages", :object => @cuepoint %>
  <%= render partial: 'cuepoints/form', locals: { f: f } %>

  <%= f.submit '登録', class: 'btn btn-success btn-centered' %>
  <%= link_to "中止", cuepoints_path %>
<% end %>
